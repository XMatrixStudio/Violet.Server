{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "ban-ts-ignore": true,
    "member-ordering": [
      true,
      {
        "alphabetize": true,
        "order": "instance-sandwich"
      }
    ],
    "no-any": true,
    "no-import-side-effect": true,
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}